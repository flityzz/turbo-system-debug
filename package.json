{
  "name": "turbo-system-debug",
  "displayName": "turbo system debug",
  "publisher": "flityzz",
  "icon": "images/tsdLOGO.png",
  "description": "Super fast system debug!",
  "version": "0.1.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/flityzz/turbo-system-debug"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "turbo-system-debug.log",
        "title": "TSD log"
      },
      {
        "command": "turbo-system-debug.removeAllSystemDebug",
        "title": "TSD remove all logs"
      }
    ],
    "keybindings": [
      {
        "command": "turbo-system-debug.log",
        "key": "ctrl+alt+d",
        "mac": "ctrl+alt+d"
      },
      {
        "command": "turbo-system-debug.removeAllSystemDebug",
        "key": "ctrl+alt+f",
        "mac": "ctrl+alt+f"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.80.0",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  }
}
